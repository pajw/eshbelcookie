{
  "manifest_version": 2,

  "name": "Eshbel Cookie Delete",
  "description": "This extension deletes Eshbel backoffice cookies.",
  "version": "1.0",

  "permissions": [
    "cookies",
    "tabs",
    "https://*/*",
    "http://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}